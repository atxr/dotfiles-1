{
  "cava":
  {
    "file": "~/.config/cava/config",
    "hunks":
    {
      "palette":
      {
        "format": "{key} = '{value}'",
        "mapping":
        [
          "gradient_color_1:gd0",
          "gradient_color_2:gd2",
          "gradient_color_3:gd4",
          "gradient_color_4:gd6",
          "gradient_color_5:gd8",
          "gradient_color_6:gd10",
          "gradient_color_7:gd12",
          "gradient_color_8:gd14"
        ]
      }
    }
  },
  "bspwm":
  {
    "file": "~/.config/bspwm/bspwmrc",
    "hunks":
    {
      "palette":
      {
        "format": "    [{key}]=\"{value}\"",
        "mapping":
        [
          "normal_border_color:cl9",
          "active_border_color:cl11",
          "focused_border_color:cl10",
          "presel_feedback_color:cl6",
          "urgent_border_color:cl13"
        ]
      }
    },
    "commands": ["if pgrep bspwm > /dev/null; then bspc wm --restart; fi"]
  },
  "qtile":
  {
    "file": "~/.config/qtile/theme.py",
    "hunks":
    {
      "palette":
      {
        "format": "  \"{key}\": \"{value}\",",
        "mapping":
        [
          "fg:fg",
          "bg:bg",
          "sel_fg:sel_fg",
          "sel_bg:sel_bg",
          "color0:cl0",
          "color1:cl1",
          "color2:cl2",
          "color3:cl3",
          "color4:cl4",
          "color5:cl5",
          "color6:cl6",
          "color7:cl7",
          "color8:cl8",
          "color9:cl9",
          "color10:cl10",
          "color11:cl11",
          "color12:cl12",
          "color13:cl13",
          "color14:cl14",
          "color15:cl15"
        ]
      }
    }
  },
  "dunst":
  {
    "file": "~/.config/dunst/dunstrc",
    "hunks":
    {
      "low":
      {
        "format": "    {key} = \"{value}\"",
        "mapping":
        [
          "background:cl8",
          "foreground:cl7"
        ]
      },
      "normal":
      {
        "format": "    {key} = \"{value}\"",
        "mapping":
        [
          "background:cl8",
          "foreground:cl6"
        ]
      },
      "critical":
      {
        "format": "    {key} = \"{value}\"",
        "mapping":
        [
          "background:cl0",
          "foreground:cl9",
          "frame_color:cl11"
        ]
      }
    },
    "commands":
    [
      "killall dunst",
      "dunstify 'CRITICAL' 'this would never go away unless clicked' -u critical -t 10000",
      "dunstify 'NORMAL' 'this goes away after 10 seconds' -u normal",
      "dunstify 'LOW' 'this goes after 3 seconds' -u low"
    ]
  },
  "alacritty":
  {
    "file": "~/.config/alacritty/alacritty.yml",
    "hunks":
    {
      "primary":
      {
        "format": "    {key}: '{value}'",
        "mapping":
        [
          "background:bg",
          "foreground:fg"
        ]
      },
      "cursor":
      {
        "format": "    {key}: '{value}'",
        "mapping":
        [
          "text:fg",
          "cursor:fg"
        ]
      },
      "normal":
      {
        "format": "    {key}: '{value}'",
        "mapping":
        [
          "black:cl0",
          "red:cl1",
          "green:cl2",
          "yellow:cl3",
          "blue:cl4",
          "magenta:cl5",
          "cyan:cl6",
          "white:cl7"
        ]
      },
      "bright":
      {
        "format": "    {key}: '{value}'",
        "mapping":
        [
          "black:cl8",
          "red:cl9",
          "green:cl10",
          "yellow:cl11",
          "blue:cl12",
          "magenta:cl13",
          "cyan:cl14",
          "white:cl15"
        ]
      }
    }
  },
  "kitty":
  {
    "file": "~/.config/kitty/kitty.conf",
    "hunks":
    {
      "palette":
      {
        "format": "{key} {value}",
        "mapping":
        [
          "foreground:fg",
          "background:bg",
          "selection_foreground:sel_fg",
          "selection_background:sel_bg",
          "cursor:fg",
          "color0:cl0",
          "color1:cl1",
          "color2:cl2",
          "color3:cl3",
          "color4:cl4",
          "color5:cl5",
          "color6:cl6",
          "color7:cl7",
          "color8:cl8",
          "color9:cl9",
          "color10:cl10",
          "color11:cl11",
          "color12:cl12",
          "color13:cl13",
          "color14:cl14",
          "color15:cl15"
        ]
      }
    }
  },
  "Xresources":
  {
    "file": "~/.config/X11/Xresources",
    "hunks":
    {
      "palette":
      {
        "format": "*{key}: {value}",
        "mapping":
        [
          "foreground:fg",
          "background:bg",
          "pointerColorForeground:sel_fg",
          "pointerColorBackground:sel_bg",
          "cursorColor:fg",
          "fadeColor:fg",
          "color0:cl0",
          "color1:cl1",
          "color2:cl2",
          "color3:cl3",
          "color4:cl4",
          "color5:cl5",
          "color6:cl6",
